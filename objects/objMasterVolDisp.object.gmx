        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Master Volume Display.
// Set Master Volume Display Colour.
draw_set_colour(intMasterVolDispColour);
// Set Master Volume Display Vertical Align.
draw_set_valign(fa_middle);
// Display Master Volume Label.
draw_set_font(fntSml);
draw_set_halign(fa_center);
var x1 = room_width*0.5;
var y1 = room_height*0.65;
draw_text(x1,y1,"VOLUME");
// Display Master Volume Bar.
var x2 = x1+room_width*0.1;
x1 -= room_width*0.1;
var y3 = y1-room_height*0.015;
var y2 = y3-room_height*0.01;
y1 = y2-room_height*0.01;
var backCol = c_black;
var minCol = intMasterVolDispColour;
var maxCol = intMasterVolDispColour;
var dir = eHBAnchorDirNames.dirLeft;
var showBack = false;
var showBorder = false;
draw_healthbar(x1,y2,x2,y3,fltMasterVol*100,backCol,minCol,maxCol,dir,showBack,showBorder);
// Display Master Volume Bar Tic Marks.
y1 += room_height*0.01;
y2 = y1 + room_height*0.005;
y3 = y1 + room_height*0.01;
// Draw Base Line.
draw_line(x1,y3,x2,y3);
// Draw End Tics.
for (var x3=x1; x3&lt;x2; x3+=x2-x1) {
    // Draw End Tic.
    draw_line(x3,y1,x3,y3);
}
// Draw Quarter Tics.
for (var x3=x1+(x2-x1)/4; x3&lt;=x2; x3+=(x2-x1)/4) {
    // Draw Quarter Tic.
    draw_line(x3,y2,x3,y3)
}

</string>
          </argument>
        </arguments>
      </action>
